//gfg problem


class Reverse
{
    String s = "Geeks";
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        // Reverse the string str
        String str1 ="";
        
        char[] arr = str.toCharArray();
        
        for(int i = arr.length-1 ; i>=0 ; i--){
            
            str1 += (arr[i]);
        }
        return str1;
    }
    
}
